<script>
    $('#version-picker').change(function (e) {
        var masterPath = '/docs';
        var versionPath = '/docs-REPLACE';

        var val = $('#version-picker').val();

                var hasArticleInThisVersion = $('#version-picker')
            .find('option[value="' + val + '"]')
            .filter('[data-version="true"]')
            .length > 0;

        if (hasArticleInThisVersion) {
            masterPath = '/docs/customize-html-export';
            versionPath = '/docs-REPLACE/customize-html-export';
        }
        
        if (val === '') {
            window.location.href = masterPath;
        } else {
            window.location.href = versionPath.replace('REPLACE', val);
        }
    });
</script>
