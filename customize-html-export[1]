<script>
    $(function () {
        function openComments() {
            $('#disqus-message').slideUp(200);

            var disqus_config = function () {
                this.page.url = 'https://dataedo.com/docs/customize-html-export';
                this.page.identifier = '/docs/customize-html-export';
            };

            (function () {
                var d = document, s = d.createElement('script');
                s.src = 'https://dataedo.disqus.com/embed.js';
                s.setAttribute('data-timestamp', +new Date());
                (d.head || d.body).appendChild(s);
            })();
        }

        function hasAnyComments() {
            var count = $('#disqus-counter').text() || 0;
            return +count > 0;
        }

        function openCommentsIfPresent() {
            if(hasAnyComments()) {
                openComments();
            }
        }

        $('#disqus-btn').click(openComments);
        setTimeout(openCommentsIfPresent, 500);
        setTimeout(openCommentsIfPresent, 1500);
        setTimeout(openCommentsIfPresent, 5000);
    });
</script>
