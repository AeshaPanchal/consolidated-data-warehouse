<script>
    $('.js-side-menu-subarticles-toggle').click(function() {
        var component = $(this).closest('.js-side-menu-subarticles');

        component.find('.js-side-menu-subarticles-toggle').toggleClass('js-active');

        component.find('.js-side-menu-subarticles-menu').slideToggle(300);
    });

    $(function() {
        var component = $('.js-menuitem-active').closest('.js-side-menu-subarticles');

        component.find('.js-side-menu-subarticles-toggle').toggleClass('js-active');

        component.find('.js-side-menu-subarticles-menu').slideDown();
    });

    $('.js-side-menu-subarticles-toggle.current').click();
</script>

<script>
    $(function() {
        // SCROLL TO ACTIVE MENU ELEMENT
        var container    = $('.sidebar-menu');
        var scrollToElem = $('.js-menuitem-active');
        var scrollToElem_height = scrollToElem[0].offsetTop;

        container.animate({
            scrollTop: scrollToElem_height - 10
        }, 0);
    })
</script>

<style>
    .js-side-menu-subarticles-toggle {
        transition: transform 0.3s;
        transform-origin: center center;
    }

    .js-side-menu-subarticles-toggle.js-active {
        transform: rotate(90deg);
    }
</style>
            </aside>

