                <div class="mb-2">
                    <div class="position-relative">
    <input id="search-docs-input" class="pr-5 form-control" type="text" placeholder="Filter by title..." />
    <div id="search-docs-loader" class="spinner-border text-primary d-none" role="status">
        <span class="sr-only">Loading...</span>
    </div>
    <div id="search-results" class="d-none border rounded shadow bg-white"></div>
</div>

<i id="external-link-icon" data-feather="external-link" class="fea icon-sm d-none ml-1"></i>


<script>
    $(document).ready(function() {
        const articlesApiRoute = "/docs/ajax/articles";
        const searchDocsInput = document.getElementById("search-docs-input");
        const searchDocsLoader = document.getElementById("search-docs-loader")
        const searchResults = document.getElementById("search-results");
        const versionId = 10;

        window.addEventListener('click', function(e){
            if (!searchResults.contains(e.target) && !searchDocsInput.contains(e.target)) {
                hideElement(searchResults);
            }
        });

        searchDocsInput.addEventListener("input", _.debounce(searchDocs, 300));
        searchDocsInput.addEventListener("focus", () => showElement(searchResults));
        searchDocs();

        function searchDocs() {
            showElement(searchDocsLoader);
            clearResultsBox();

            const inputValue = searchDocsInput.value;
            if (!inputValue) {
                hideElement(searchDocsLoader);
                return;
            }

            let uri = `${articlesApiRoute}?q=${inputValue}`;
            if (versionId) {
                uri += `&versionId=${versionId}`;
            }

            fetch(uri, {
                credentials: 'include'
            })
                .then(res => res.json())
                .then(articles => {
                    if (articles.length === 0) {
                        searchResults.appendChild(createNoResultsFoundElement());
                    }

                    for (const articleItem of articles) {
                        const resultItem = createResultElement(articleItem);
                        searchResults.appendChild(resultItem);
                    }

                    hideElement(searchDocsLoader);
                });
        }

        function showElement(element) {
            if (element.classList.contains("d-none")) {
                element.classList.remove("d-none");
            }
        }

        function hideElement(element) {
            if (!element.classList.contains("d-none")) {
                element.classList.add("d-none");
            }
        }

        function clearResultsBox() {
            while (searchResults.firstChild) {
                searchResults.firstChild.remove();
            }
        }

        function createResultElement(article) {
            const resultItem = document.createElement("div");

            resultItem.classList.add("p-2", "result", "text-primary");

            if (!article.is_visible) {
                resultItem.style.opacity = "0.5";
            }

            resultItem.innerText = article.title;

            if (article.is_redirect) {
                const icon = document.getElementById("external-link-icon").cloneNode(true);
                icon.classList.remove("d-none");
                resultItem.innerHTML += icon.outerHTML;
            }

            resultItem.addEventListener("click", () => window.location.href = article.url);

            return resultItem;
        }

        function createNoResultsFoundElement() {
            const div = document.createElement("div");
            div.classList.add("p-2", "text-center");
            div.innerText = "No results found"
            return div;
        }
    });
</script>

